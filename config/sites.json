          {
            "action": "WAIT",
            "target": "",
            "optional": false,
            "value": 0.1
          },
          {
            "action": "CLICK",
            "target": "input_box",
            "optional": false,
            "value": null
          },
          {
            "action": "WAIT",
            "target": "",
            "optional": false,
            "value": 0.1
          },
          {
            "action": "FILL_INPUT",
            "target": "input_box",
            "optional": false,
            "value": null
          },
          {
            "action": "CLICK",
            "target": "send_btn",
            "optional": false,
            "value": null
          },
          {
            "action": "STREAM_WAIT",
            "target": "result_container",
            "optional": false,
            "value": null
